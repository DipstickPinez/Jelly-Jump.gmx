{
    directlast = move //finds the last direction faced
}
if sign(vel) != 0 //if you are moving
{
    if move == 0 or move == -sign(vel) //but you are not inputting something or your speed is the opposite of the direction you inputted
    {
    vel = 0 //set speed to 0
    }   
}
direct = move //this is the constant direction
if sign(move) = 0 //reset accelstart if you are not inputting
{
accelstart = 0
}
}
